		"@types/react-dom": "^19.0.1",
		"cross-env": "^7.0.3",
		"msw": "^2.7.3",
		"playwright": "^1.51.1",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.19.3",
		"typescript": "^5.7.2",
		"vite": "^5.4.11",
		"vite-tsconfig-paths": "^5.1.4"
