{
	"name": "ai-resume-generator",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"build:dev": "react-router build --watch --profile --host 0.0.0.0 --sourcemapClient true --sourcemapServer true --minify false --logLevel info",
		"dev": "cross-env NODE_ENV=development tsx server.ts",
		"dev:msw": "cross-env MSW_ENABLED=true NODE_ENV=development PORT=4000 tsx server.ts",
		"start": "tsx server.ts",
		"start:prod:msw": "cross-env MSW_ENABLED=true NODE_ENV=production PORT=4000 tsx server.ts",
		"typecheck": "react-router typegen && tsc -b",
		"test:e2e": "playwright test --reporter=line",
		"test:e2e:ui": "playwright test --ui",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check --write .",
		"lint:format": "biome format --write .",
		"lint:lint": "biome lint --write ."
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.40.1",
		"@conform-to/react": "^1.4.0",
		"@conform-to/zod": "^1.4.0",
		"@emotion/styled": "^11.14.0",
		"@mdxeditor/editor": "^3.31.1",
		"@react-router/express": "^7.5.3",
		"@react-router/node": "^7.5.3",
		"@types/better-sqlite3": "^7.6.13",
		"better-sqlite3": "^11.9.1",
		"clsx": "^2.1.1",
		"compression": "^1.8.0",
		"dotenv": "^16.5.0",
		"express": "^5.1.0",
		"isbot": "^5.1.27",
		"json-schema-faker": "^0.5.9",
		"morgan": "^1.10.0",
		"openai": "^4.96.2",
		"puppeteer": "^24.7.2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-markdown": "^10.1.0",
		"react-router": "^7.5.3",
		"tailwind-merge": "^3.2.0",
		"zod": "^3.24.3",
		"zod-to-json-schema": "^3.24.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@playwright/test": "^1.52.0",
		"@react-router/dev": "^7.5.3",
		"@tailwindcss/vite": "^4.1.4",
		"@types/compression": "^1.7.5",
		"@types/express": "^5.0.1",
		"@types/express-serve-static-core": "^5.0.6",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.15.3",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@vitest/ui": "^3.1.2",
		"cross-env": "^7.0.3",
		"lefthook": "^1.11.12",
		"msw": "^2.7.5",
		"playwright": "^1.52.0",
		"tailwindcss": "^4.1.4",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"vite": "^6.3.4",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.2"
	},
	"engines": {
		"node": "20.17.0",
		"pnpm": "10.6.3"
	},
	"msw": {
		"workerDirectory": ["public"]
	},
	"pnpm": {
		"onlyBuiltDependencies": ["better-sqlite3"],
		"ignoredBuiltDependencies": ["@biomejs/biome", "core-js", "esbuild", "msw"]
	}
}
