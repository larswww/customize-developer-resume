	"dependencies": {
		"@react-router/express": "^7.4.0",
		"@react-router/node": "^7.4.0",
		"compression": "^1.7.5",
		"dotenv": "^16.4.1",
		"express": "^4.21.1",
		"isbot": "^5.1.17",
		"morgan": "^1.10.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-markdown": "^9.0.1",
		"react-router": "^7.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@playwright/test": "^1.51.1",
		"@react-router/dev": "^7.4.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/compression": "^1.7.5",
