{
	"name": "ai-resume-generator",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"build:dev": "react-router build --watch --profile --host 0.0.0.0 --sourcemapClient true --sourcemapServer true --minify false --logLevel info",
		"dev": "cross-env DB_NAME=dev NODE_ENV=development tsx server.ts",
		"dev:msw": "cross-env DB_NAME=e2e MSW_ENABLED=true NODE_ENV=development PORT=4000 tsx server.ts",
		"worker": "tsx app/services/queue/resumeWorker.server.ts",
		"start": "node --experimental-strip-types --experimental-transform-types server.ts",
		"start:prod:msw": "cross-env DB_NAME=e2e MSW_ENABLED=true NODE_ENV=production PORT=4000 tsx server.ts",
		"typecheck": "react-router typegen && tsc -b",
		"test:e2e": "cross-env DB_NAME=e2e MSW_ENABLED=true OPENAI_API_KEY=mock playwright test --reporter=line",
		"test:e2e:ui": "cross-env DB_NAME=e2e MSW_ENABLED=true OPENAI_API_KEY=mock playwright test --ui",
		"test": "cross-env DB_NAME=unit vitest run",
		"test:watch": "cross-env DB_NAME=unit vitest",
		"test:ui": "cross-env DB_NAME=unit vitest --ui",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check --write --organize-imports-enabled=true .",
		"lint:format": "biome format --write .",
		"lint:lint": "biome lint --write ."
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.51.0",
		"@conform-to/react": "^1.6.0",
		"@conform-to/zod": "^1.6.0",
		"@emotion/styled": "^11.14.0",
		"@mdxeditor/editor": "^3.32.3",
		"@radix-ui/react-collapsible": "^1.1.11",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tooltip": "^1.2.7",
		"@react-router/express": "^7.6.0",
		"@react-router/node": "^7.6.0",
		"@types/better-sqlite3": "^7.6.13",
		"@types/lodash": "^4.17.17",
		"better-sqlite3": "^11.10.0",
		"bullmq": "^5.53.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"compression": "^1.8.0",
		"date-fns": "^4.1.0",
		"dotenv": "^16.5.0",
		"express": "^5.1.0",
		"isbot": "^5.1.28",
		"json-schema-faker": "^0.5.9",
		"lodash": "^4.17.21",
		"lucide-react": "^0.511.0",
		"morgan": "^1.10.0",
		"openai": "^4.102.0",
		"puppeteer": "^24.9.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-markdown": "^10.1.0",
		"react-router": "^7.6.0",
		"tailwind-merge": "^3.3.0",
		"zod": "^3.25.20",
		"zod-to-json-schema": "^3.24.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@playwright/test": "^1.52.0",
		"@react-router/dev": "^7.6.0",
		"@tailwindcss/vite": "^4.1.7",
		"@types/compression": "^1.8.0",
		"@types/express": "^5.0.2",
		"@types/express-serve-static-core": "^5.0.6",
		"@types/morgan": "^1.9.9",
		"@types/node": "^22.15.21",
		"@types/react": "^19.1.5",
		"@types/react-dom": "^19.1.5",
		"@vitest/ui": "^3.1.4",
		"cross-env": "^7.0.3",
		"happy-dom": "^17.4.7",
		"lefthook": "^1.11.13",
		"msw": "^2.8.4",
		"playwright": "^1.52.0",
		"tailwindcss": "^4.1.7",
		"tsx": "^4.19.4",
		"tw-animate-css": "^1.3.0",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.4"
	},
	"engines": {
		"node": "22.16.0",
		"pnpm": "10.11.0"
	},
	"packageManager": "pnpm@10.11.0",
	"msw": {
		"workerDirectory": ["public"]
	},
	"pnpm": {
		"onlyBuiltDependencies": ["better-sqlite3"],
		"ignoredBuiltDependencies": ["@biomejs/biome", "core-js", "esbuild", "msw"]
	}
}
