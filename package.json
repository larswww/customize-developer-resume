{
	"name": "ai-resume-generator",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"dev": "cross-env NODE_ENV=development tsx server.ts",
		"dev:msw": "cross-env MSW_ENABLED=true tsx server.ts",
		"start": "tsx server.ts",
		"typecheck": "react-router typegen && tsc -b",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check --write .",
		"lint:format": "biome format --write .",
		"lint:lint": "biome lint --write ."
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.39.0",
		"@emotion/styled": "^11.14.0",
		"@mdxeditor/editor": "^3.29.1",
		"@radix-ui/react-collapsible": "^1.1.3",
		"@react-router/express": "^7.4.0",
		"@react-router/node": "^7.4.0",
		"@types/better-sqlite3": "^7.6.12",
		"better-sqlite3": "^11.9.1",
		"clsx": "^2.1.1",
		"compression": "^1.7.5",
		"dotenv": "^16.4.1",
		"express": "^4.21.1",
		"html2canvas": "^1.4.1",
		"isbot": "^5.1.17",
		"jspdf": "^3.0.1",
		"morgan": "^1.10.0",
		"openai": "^4.91.1",
		"pdfjs-dist": "^5.1.91",
		"puppeteer-core": "^24.5.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-markdown": "^9.0.1",
		"react-router": "^7.4.0",
		"tailwind-merge": "^3.1.0",
		"zod": "^3.24.2",
		"zod-to-json-schema": "^3.24.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@playwright/test": "^1.51.1",
		"@react-router/dev": "^7.4.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/compression": "^1.7.5",
		"@types/express": "^5.0.0",
		"@types/express-serve-static-core": "^5.0.1",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.1",
		"@vitest/ui": "^3.1.1",
		"cross-env": "^7.0.3",
		"msw": "^2.7.3",
		"playwright": "^1.51.1",
		"react-router-devtools": "^1.1.10",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.19.3",
		"typescript": "^5.7.2",
		"vite": "^5.4.11",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.1"
	},
	"engines": {
		"node": "20.17.0",
		"pnpm": "10.6.3"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"better-sqlite3"
		],
		"ignoredBuiltDependencies": [
			"@biomejs/biome",
			"core-js",
			"esbuild",
			"msw"
		]
	}
}
