{
	"name": "ai-resume-generator",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"build:dev": "react-router build --watch --profile --host 0.0.0.0 --sourcemapClient true --sourcemapServer true --minify false --logLevel info",
		"dev": "cross-env DB_NAME=dev.db NODE_ENV=development tsx server.ts",
		"dev:msw": "cross-env DB_NAME=e2e.db MSW_ENABLED=true NODE_ENV=development PORT=4000 tsx server.ts",
		"worker": "tsx app/services/queue/resumeWorker.server.ts",
		"start": "tsx server.ts",
		"start:prod:msw": "cross-env DB_NAME=e2e.db MSW_ENABLED=true NODE_ENV=production PORT=4000 tsx server.ts",
		"typecheck": "react-router typegen && tsc -b",
		"test:e2e": "cross-env DB_NAME=e2e.db MSW_ENABLED=true OPENAI_API_KEY=mock playwright test --reporter=line",
		"test:e2e:ui": "cross-env DB_NAME=e2e.db MSW_ENABLED=true OPENAI_API_KEY=mock playwright test --ui",
		"test": "cross-env DB_NAME=unit.db vitest run",
		"test:watch": "cross-env DB_NAME=unit.db vitest",
		"test:ui": "cross-env DB_NAME=unit.db vitest --ui",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check --write --organize-imports-enabled=true .",
		"lint:format": "biome format --write .",
		"lint:lint": "biome lint --write ."
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.39.0",
		"@conform-to/react": "^1.4.0",
		"@conform-to/zod": "^1.4.0",
		"@emotion/styled": "^11.14.0",
		"@mdxeditor/editor": "^3.29.1",
		"@radix-ui/react-dialog": "^1.1.13",
		"@radix-ui/react-separator": "^1.1.6",
		"@radix-ui/react-slot": "^1.2.2",
		"@radix-ui/react-tooltip": "^1.2.6",
		"@react-router/express": "^7.4.0",
		"@react-router/node": "^7.4.0",
		"@types/better-sqlite3": "^7.6.12",
		"@types/lodash": "^4.17.16",
		"better-sqlite3": "^11.9.1",
		"bullmq": "^5.52.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"compression": "^1.7.5",
		"dotenv": "^16.4.1",
		"express": "^4.21.1",
		"isbot": "^5.1.17",
		"json-schema-faker": "^0.5.9",
		"lodash": "^4.17.21",
		"lucide-react": "^0.507.0",
		"morgan": "^1.10.0",
		"openai": "^4.91.1",
		"puppeteer": "^24.7.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-markdown": "^9.0.1",
		"react-router": "^7.4.0",
		"tailwind-merge": "^3.2.0",
		"zod": "^3.24.2",
		"zod-to-json-schema": "^3.24.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@playwright/test": "^1.51.1",
		"@react-router/dev": "^7.4.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/compression": "^1.7.5",
		"@types/express": "^5.0.0",
		"@types/express-serve-static-core": "^5.0.1",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.1",
		"@vitest/ui": "^3.1.1",
		"cross-env": "^7.0.3",
		"lefthook": "^1.11.11",
		"msw": "^2.7.3",
		"playwright": "^1.51.1",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.19.3",
		"tw-animate-css": "^1.2.9",
		"typescript": "^5.7.2",
		"vite": "^5.4.11",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.1"
	},
	"engines": {
		"node": "20.17.0",
		"pnpm": "10.6.3"
	},
	"msw": {
		"workerDirectory": ["public"]
	},
	"pnpm": {
		"onlyBuiltDependencies": ["better-sqlite3"],
		"ignoredBuiltDependencies": ["@biomejs/biome", "core-js", "esbuild", "msw"]
	}
}
